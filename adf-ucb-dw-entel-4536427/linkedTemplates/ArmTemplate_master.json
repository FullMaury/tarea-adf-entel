{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-ucb-dw-entel-4536427"
		},
		"Is_sa_datos_entel_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Is_sa_datos_entel'"
		},
		"Is_sql_dw_entel_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Is_sql_dw_entel'"
		},
		"Is_sql_dw_entel_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-ucb-sisinfo-entel-delta.database.windows.net"
		},
		"Is_sql_dw_entel_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dw_entel_delta"
		},
		"Is_sql_dw_entel_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "franklin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-ucb-dw-entel-4536427_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Is_sa_datos_entel_connectionString": {
						"value": "[parameters('Is_sa_datos_entel_connectionString')]"
					},
					"Is_sql_dw_entel_password": {
						"value": "[parameters('Is_sql_dw_entel_password')]"
					},
					"Is_sql_dw_entel_properties_typeProperties_server": {
						"value": "[parameters('Is_sql_dw_entel_properties_typeProperties_server')]"
					},
					"Is_sql_dw_entel_properties_typeProperties_database": {
						"value": "[parameters('Is_sql_dw_entel_properties_typeProperties_database')]"
					},
					"Is_sql_dw_entel_properties_typeProperties_userName": {
						"value": "[parameters('Is_sql_dw_entel_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-ucb-dw-entel-4536427_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-ucb-dw-entel-4536427_ArmTemplate_0')]"
			]
		}
	]
}