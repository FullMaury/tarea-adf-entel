{
	"name": "df_hechos_llamadas",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_llamadas",
						"type": "DatasetReference"
					},
					"name": "silver"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_fecha",
						"type": "DatasetReference"
					},
					"name": "dimFecha"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_numero",
						"type": "DatasetReference"
					},
					"name": "dimNumero"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_radio_base",
						"type": "DatasetReference"
					},
					"name": "dimRadibase"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_registro",
						"type": "DatasetReference"
					},
					"name": "dimRegistro"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_servicio",
						"type": "DatasetReference"
					},
					"name": "dimServicio"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_hechos_llamadas",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "lkFecha"
				},
				{
					"name": "lkNumeroA"
				},
				{
					"name": "lkRadiobaseA"
				},
				{
					"name": "lkRegistro"
				},
				{
					"name": "lkServicio"
				},
				{
					"name": "select1"
				},
				{
					"name": "lkNumeroB"
				},
				{
					"name": "select2"
				},
				{
					"name": "select3"
				},
				{
					"name": "select4"
				},
				{
					"name": "lkRadiobaseB"
				},
				{
					"name": "select5"
				}
			],
			"scriptLines": [
				"source(output(",
				"          SERVICIO as string,",
				"          REGISTRO as string,",
				"          NUMERO_A as string,",
				"          IMEI_A as string,",
				"          RBS_UTILIZADA_A as string,",
				"          CELDA_A as string,",
				"          LATITUD_A as decimal(9,6),",
				"          LONGITUD_A as decimal(9,6),",
				"          NUMERO_B as string,",
				"          IMEI_B as string,",
				"          RBS_UTILIZADA_B as string,",
				"          CELDA_B as string,",
				"          LATITUD_B as decimal(9,6),",
				"          LONGITUD_B as decimal(9,6),",
				"          FECHA as timestamp,",
				"          DURACION as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> silver",
				"source(output(",
				"          id_fecha as integer,",
				"          fecha as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimFecha",
				"source(output(",
				"          id_numero as integer,",
				"          numero as string,",
				"          imei as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimNumero",
				"source(output(",
				"          id_radio_base as integer,",
				"          radio_base as string,",
				"          celda as string,",
				"          latitud as decimal(18,0),",
				"          longitud as decimal(18,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimRadibase",
				"source(output(",
				"          id_registro as integer,",
				"          registro as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimRegistro",
				"source(output(",
				"          id_servicio as integer,",
				"          servicio as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimServicio",
				"silver, dimFecha lookup(silver@FECHA == dimFecha@fecha,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lkFecha",
				"lkFecha, dimNumero lookup(NUMERO_A == numero,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lkNumeroA",
				"select3, dimRadibase lookup(RBS_UTILIZADA_A == radio_base,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lkRadiobaseA",
				"select5, dimRegistro lookup(silver@REGISTRO == dimRegistro@registro,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lkRegistro",
				"lkRegistro, dimServicio lookup(silver@SERVICIO == dimServicio@servicio,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lkServicio",
				"lkServicio select(mapColumn(",
				"          DURACION,",
				"          id_fecha,",
				"          id_numero = id_numero_b,",
				"          id_radio_base = id_radio_base_a,",
				"          id_registro,",
				"          id_servicio",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select2, dimNumero lookup(NUMERO_B == dimNumero@numero,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lkNumeroB",
				"lkNumeroA select(mapColumn(",
				"          SERVICIO,",
				"          REGISTRO,",
				"          NUMERO_A,",
				"          IMEI_A,",
				"          RBS_UTILIZADA_A,",
				"          CELDA_A,",
				"          LATITUD_A,",
				"          LONGITUD_A,",
				"          NUMERO_B,",
				"          IMEI_B,",
				"          RBS_UTILIZADA_B,",
				"          CELDA_B,",
				"          LATITUD_B,",
				"          LONGITUD_B,",
				"          FECHA = silver@FECHA,",
				"          DURACION,",
				"          id_fecha,",
				"          fecha = dimFecha@fecha,",
				"          id_numero,",
				"          numero,",
				"          imei",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"lkNumeroB select(mapColumn(",
				"          SERVICIO,",
				"          REGISTRO,",
				"          NUMERO_A,",
				"          IMEI_A,",
				"          RBS_UTILIZADA_A,",
				"          CELDA_A,",
				"          LATITUD_A,",
				"          LONGITUD_A,",
				"          NUMERO_B,",
				"          IMEI_B,",
				"          RBS_UTILIZADA_B,",
				"          CELDA_B,",
				"          LATITUD_B,",
				"          LONGITUD_B,",
				"          FECHA,",
				"          DURACION,",
				"          id_fecha,",
				"          id_numero_a = id_numero,",
				"          id_numero_b = id_numero",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"lkRadiobaseA select(mapColumn(",
				"          SERVICIO,",
				"          REGISTRO,",
				"          RBS_UTILIZADA_B,",
				"          CELDA_B,",
				"          LATITUD_B,",
				"          LONGITUD_B,",
				"          FECHA = select3@FECHA,",
				"          DURACION,",
				"          id_fecha,",
				"          fecha = select3@fecha,",
				"          id_numero_a,",
				"          numero,",
				"          imei,",
				"          id_numero_b,",
				"          id_radio_base_a = id_radio_base",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select4",
				"select4, dimRadibase lookup(RBS_UTILIZADA_B == radio_base,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lkRadiobaseB",
				"lkRadiobaseB select(mapColumn(",
				"          SERVICIO,",
				"          REGISTRO,",
				"          RBS_UTILIZADA_B,",
				"          CELDA_B,",
				"          LATITUD_B,",
				"          LONGITUD_B,",
				"          id_fecha,",
				"          id_numero_a,",
				"          id_numero_b,",
				"          id_radio_base_a,",
				"          radio_base = select4@radio_base",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> select5",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_llamadas as integer,",
				"          duracion as timestamp,",
				"          id_servicio as integer,",
				"          id_registro as integer,",
				"          id_numero_a as integer,",
				"          id_radio_base_a as integer,",
				"          id_numero_b as integer,",
				"          id_radio_base_b as integer,",
				"          id_fecha as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          duracion = DURACION,",
				"          id_servicio,",
				"          id_registro,",
				"          id_fecha",
				"     )) ~> sink1"
			]
		}
	}
}