{
	"name": "df_hechos_datos",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_datos",
						"type": "DatasetReference"
					},
					"name": "silver"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_fecha",
						"type": "DatasetReference"
					},
					"name": "dimFecha"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_numero",
						"type": "DatasetReference"
					},
					"name": "dimNumero"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_radio_base",
						"type": "DatasetReference"
					},
					"name": "dimRadiobase"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_hechos_datos",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "lkFechaInicio"
				},
				{
					"name": "lkNumero"
				},
				{
					"name": "lkRadiobase"
				},
				{
					"name": "select1"
				},
				{
					"name": "lkFechaFin"
				}
			],
			"scriptLines": [
				"source(output(",
				"          FECHA_INICIO as timestamp,",
				"          FECHA_FIN as timestamp,",
				"          NUMERO as string,",
				"          IMEI as string,",
				"          RBS_UTILIZADA as string,",
				"          CELDA as string,",
				"          LATITUD as decimal(9,6),",
				"          LONGITUD as decimal(9,6),",
				"          DATOS_SUBIDOS as integer,",
				"          DATOS_DESCARGADOS as integer,",
				"          VOLUMEN_TOTAL as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> silver",
				"source(output(",
				"          id_fecha as integer,",
				"          fecha as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimFecha",
				"source(output(",
				"          id_numero as integer,",
				"          numero as string,",
				"          imei as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimNumero",
				"source(output(",
				"          id_radio_base as integer,",
				"          radio_base as string,",
				"          celda as string,",
				"          latitud as decimal(18,0),",
				"          longitud as decimal(18,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimRadiobase",
				"silver, dimFecha lookup(FECHA_INICIO == fecha",
				"     && FECHA_FIN == fecha,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lkFechaInicio",
				"lkFechaFin, dimNumero lookup(silver@NUMERO == dimNumero@numero,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lkNumero",
				"lkNumero, dimRadiobase lookup(RBS_UTILIZADA == radio_base,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lkRadiobase",
				"lkRadiobase select(mapColumn(",
				"          DATOS_SUBIDOS,",
				"          DATOS_DESCARGADOS,",
				"          VOLUMEN_TOTAL,",
				"          id_fecha,",
				"          id_numero,",
				"          id_radio_base",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"lkFechaInicio lookup(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lkFechaFin",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_datos as integer,",
				"          subidos as integer,",
				"          descargador as integer,",
				"          total as integer,",
				"          id_fecha as integer,",
				"          id_numero as integer,",
				"          id_radio_base as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          subidos = DATOS_SUBIDOS,",
				"          descargador = DATOS_DESCARGADOS,",
				"          total = VOLUMEN_TOTAL,",
				"          id_fecha,",
				"          id_numero,",
				"          id_radio_base",
				"     )) ~> sink1"
			]
		}
	}
}