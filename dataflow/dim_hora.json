{
	"name": "dim_hora",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_llamadas",
						"type": "DatasetReference"
					},
					"name": "silver"
				}
			],
			"sinks": [
				{
					"name": "dimhora"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          hora as timestamp,",
				"          hora24 as integer,",
				"          hora12 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT DISTINCT\\nhora AS hora,\\nDATEPART(hour, hora) AS hora24,\\nformat(hora, \\'hh\\') AS hora12\\nFROM SILVER.LLAMADAS_TIGO',",
				"     format: 'query') ~> silver",
				"silver sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          hora,",
				"          hora24",
				"     ),",
				"     format: 'table') ~> dimhora"
			]
		}
	}
}